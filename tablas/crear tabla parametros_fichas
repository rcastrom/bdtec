create table "ITE".parametros_fichas (
    id                 serial        not null,
	fichas             "T_periodo"   not null  ,
	activo             boolean       not null  ,
	inicio_prope       date          null,
	fin_prope          date          null,
	entrega            date          null,
	termina            date          null,    
    created_at                      timestamp without time zone          null  ,
    updated_at                      timestamp without time zone          null  ,                       
);

