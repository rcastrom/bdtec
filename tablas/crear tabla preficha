CREATE TABLE "ITE".preficha (
	no_recibo                       int                           not null  ,
	fecha_registro                  date                          null  ,
	instituto                       char(50)                      null  ,
	apellido_paterno                "ITE"."T_apellidos_persona"   null  ,
	apellido_materno                "ITE"."T_apellidos_persona"   not null  ,
	nombre_aspirante                varchar(255)                  not null  ,
	nip                             int                           not null  ,
	fecha_nacimiento                date                          null  ,
	sexo                            "ITE"."T_sexo"                null   ,
	nacionalidad                    char(1)                       null  ,
	especifique_extranjero          char(20)                      null  ,
	curp                            "ITE"."T_curp"                null  ,
	carrera_opcion_1                "ITE"."T_carrera"             null  ,
	entidad_federativa_prepa        "ITE"."T_lugar_nac"           null  ,
	clave_preparatoria              varchar(15)                   null  ,
	agnio_egreso                    int                           null  ,
	promedio_general                "ITE"."T_promedio"            null  ,
	calle_no                        varchar(80)                   null  ,
	entidad_federativa              "ITE"."T_lugar_nac"           null  ,
	municipio                       "ITE"."T_ciudad"              null  ,
	codigo_postal                   varchar(5)                    null  ,
	colonia_aspirante               varchar(40)                   null  ,
	correo_electronico              varchar(60)                   null  ,
	telefono                        varchar(30)                   null  ,
	estado_civil                    "ITE"."T_estado_civil"        null   ,
	capacidad_diferente             char(1)                       null  ,
	tienes_beca                     char(1)                       null  ,
	quien_otorgo                    char(30)                      null  ,
	zona_procedencia                char(1)                       null  ,
	especifique_zona_procedencia    char(30)                      null  ,
	programa_oportunidades          char(1)                       null  ,
	apellido_paterno_padre          "ITE"."T_apellidos_persona"   null  ,
	apellido_materno_padre          "ITE"."T_apellidos_persona"   null  ,
	nombre_padre_aspirante          varchar(50)                   null  ,
	vive_padre                      char(1)                       null  ,
	apellido_paterno_madre          "ITE"."T_apellidos_persona"   null  ,
	apellido_materno_madre          "ITE"."T_apellidos_persona"   null  ,
	nombre_madre_aspirante          varchar(50)                   null  ,
	vive_madre                      char(1)                       null  ,
	fecha_atencion                  date                          null  ,
	hora_atencion                   char(50)                      null  ,
	no_solicitud                    int                           not null  ,
	periodo                         "ITE"."T_periodo"             not null  ,
	itmin                           char(9)                       null  ,
	folio_ceneval                   int                           null  ,
	created_at                      timestamp without time zone   null  ,
	updated_at                      timestamp without time zone   null   
);

